;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.gardenwalk.edu. admin.gardenwalk.edu. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
	IN	NS	ns.gardenwalk.edu.

; name servers - A records
ns.gardenwalk.edu.	IN	A	192.168.1.11

; 192.168.1.0/16 - A records
host.gardenwalk.edu.	IN	A	192.168.1.10
