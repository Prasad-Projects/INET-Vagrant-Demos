;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.gardenwalk.edu. admin.gardenwalk.edu. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
	IN	NS	ns.gardenwalk.edu

;PTR records
11.1	IN	PTR	ns.gardenwalk.edu.	; 192.168.1.11
10.1	IN	PTR	host.gardenwalk.edu.	; 192.168.1.10	
